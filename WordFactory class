import java.util.Random;

public class WordFactory {
	String[] WordList = {
			"Dog", 
			"Cat", 
			"Bear", 
			"Cow",
			"Fish",
			"Horse",
			"Computer",
			"Science"
			};
	
	public String makeWord() {
		Random rand = new Random();
		int n = rand.nextInt(WordList.length);
		return WordList[n];
	}
}
